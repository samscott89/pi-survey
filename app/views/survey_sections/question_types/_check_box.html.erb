<%= f.label q.subtext, class: "control-label" %>
<% q.question_options.each do |opt| %>
<%  next if opt.option_choice.id == 54 #hacky method to get blanks hidden %>
<div class="checkbox">
	<%= f.check_box :option_id, {multiple: true}, opt.id, "" %>
	<%= f.label opt.option_choice.choice_name.capitalize %>
</div>
<% end %>