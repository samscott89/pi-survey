<%= f.label q.subtext, class: "control-label" %>
<%= f.fields_for :answer_options, AnswerOption.new do |f2| %>
<% q.question_options.each do |opt| %>
<div class="checkbox">
	<%= f2.label :option_id do %>
	<%= f2.check_box :option_id, {multiple: true}, opt.id, nil %> <%= opt.option_choice.choice_name.capitalize %>
	<% end %>
</div>
<% end %>
<% end %>