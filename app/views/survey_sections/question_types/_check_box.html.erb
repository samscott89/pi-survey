<%= f.label q.subtext, class: "control-label" %>
<% q.question_options.each do |opt| %>
<%= f.fields_for :answer_options do |f2| %>
<% next if f2.object.option_id != opt.id %>
<div class="checkbox">
	<%= f2.label :option_id do %>
	<%= f2.check_box :option_id, {multiple: false}, opt.id, nil %> <%= opt.option_choice.choice_name.capitalize %>
	<% end %>
</div>
<% end %>
<% end %>