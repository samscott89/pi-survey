<%= f.label q.subtext, class: "control-label" %>
<%= f.hidden_field :option_id, value: nil %>
<% q.question_options.each do |opt| %>
<%  next if opt.option_choice.id == 57 #hacky method to get blanks hidden %>
<div class="radio">
	<%= f.radio_button :option_id, opt.id	 %>
	<%= f.label opt.option_choice.choice_name.capitalize  %>
</div>
<% end %>